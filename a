def SumTwoNumbers(x, y):
  print(x+y)
  
SumTwoNumbers(5,6)

2.piem

def printModelis(n):
    for i in range(n):
        print('=======')
        print(' -x+x-')
        print('=======')

name = input('What is your name?: ')
printModelis(1)
age = input('How old are you?: ')
printModelis(2)
drink = input('What is yout favorite drink?: ')
printModelis(3)
print(f'Hello {name}! You are {age} y.o.\nYour favorite drink is {drink}.')

 3.piem
import random

def cube():
    result = random.randint(1, 6)
    if result == 1:
        return "One"
    elif result == 2:
        return "Two"
    elif result == 3:
        return "Three"
    elif result == 4:
        return "Four"
    elif result == 5:
        return "Five"
    else:
        return "Six"

game_result = cube()
print(game_result)
