contacts = []

name = input('Enter name: ')
surname = input('Enter surname: ')
number = input('Enter number: ')
email = input('Enter email: ')

contact = {
    'name': name, 
    'surname': surname, 
    'number': number,
    'email': email
}

contacts.append(contact)

