def nice_shapes():
    print('=======\n -x+x-\n=======')

atb = []
for i in ['What is your name?:','How old are you?:','How old are you?:']:
    atb.append(input(i))
    nice_shapes()
print(f'Hello {name}! You are {age} y.o.\nYour favorite drink is {drink}.')

